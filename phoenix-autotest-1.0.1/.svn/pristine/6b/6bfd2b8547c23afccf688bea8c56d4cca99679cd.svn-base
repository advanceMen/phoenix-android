<script type="text/javascript"> 
function myFun(n){
  var attValue=$("#image"+n).attr("src");

if(attValue == "../images/expended_image.gif"){
    $("#image"+n).attr("src","../images/collapsed_image.gif");
  }else{
    $("#image"+n).attr("src","../images/expended_image.gif");
  }
    $(".nowamagic"+n).slideToggle("fast");
}

function PlanFun(n){
  var attValue=$("#img"+n).attr("src");

if(attValue == "../images/expended_image.gif"){
    $("#img"+n).attr("src","../images/collapsed_image.gif");
  }else{
    $("#img"+n).attr("src","../images/expended_image.gif");
  }
    $(".planmagic"+n).slideToggle("fast");
}

</script>

</head>

